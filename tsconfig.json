{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
      "strictNullChecks": true,
  "baseUrl": ".",
  "resolveJsonModule": true,
  "allowSyntheticDefaultImports": true,
  "paths": {
    "@/*": ["src/*"],
  },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["./src"]
}